{{ if or (eq .chezmoi.os "darwin") (eq .chezmoi.os "linux") }}
[".config/nvim"]
    type = "git-repo"
    url = "https://github.com/LaurentHrt/nvim.git"
    exact = true
    stripComponents = 1
[".config/tmux/plugins/tpm"]
    type = "git-repo"
    url = "https://github.com/tmux-plugins/tpm"
    exact = true
    stripComponents = 1
{{ end }}
